{
    "identifier": "com.anycowork.desktop",
    "productName": "AnyCowork",
    "version": "0.1.0",
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build",
        "devUrl": "http://localhost:5173",
        "frontendDist": "../dist"
    },
    "app": {
        "security": {
            "csp": null,
            "capabilities": [
                "default"
            ]
        },
        "windows": [
            {
                "title": "AnyCowork",
                "width": 1280,
                "height": 800,
                "resizable": true,
                "fullscreen": false,
                "decorations": false,
                "transparent": true,
                "hiddenTitle": true,
                "titleBarStyle": "Overlay"
            }
        ]
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    },
    "plugins": {
        "shell": {
            "open": true
        }
    }
}